root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 4
indent_style = space
insert_final_newline = false
max_line_length = 120
tab_width = 4
ij_continuation_indent_size = 8

[*.java]
ij_java_imports_layout = *, |, javax.**, java.**, |, $*

[{*.markdown,*.md}]
max_line_length = 90

[{*.gradle.kts,*.kt,*.kts,*.main.kts,*.space.kts}]
ij_kotlin_imports_layout = *, java.**, javax.**, kotlin.**, ^
